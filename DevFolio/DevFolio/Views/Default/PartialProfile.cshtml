@model List<DevFolio.Models.TblProfile>

@foreach (var x in Model)
{
    <div class="row">
        <div class="col-sm-6 col-md-5">
            <div class="about-img">
                <img src="~/Templates/devfolio-master/img/student.png" class="img-fluid rounded b-shadow-a" alt="">
            </div>
        </div>
        <div class="col-sm-6 col-md-7">
            <div class="about-info">
                <p><span class="title-s">AdSoyad: </span> <span>@x.NameSurName</span></p>
                <p><span class="title-s">Unvan: </span> <span>@x.Title</span></p>
                <p><span class="title-s">Email: </span> <span>@x.Email</span></p>
                <p><span class="title-s">Phone: </span> <span>@x.Phone</span></p>
            </div>
        </div>
    </div>
}
