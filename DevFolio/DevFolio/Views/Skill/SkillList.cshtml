@model List<DevFolio.Models.TblSkill>

@{
    ViewBag.Title = "SkillList";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>YeteneklerListesi</h2>


<table class="table table-bordered">
    <tr>
        <th></th>
        <th>Yetenek Baslik</th>
        <th>Yetenek ORani</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.SkillID</td>
            <td>@x.SkillTitle</td>
            <td>@x.SkillValue</td>
            <td><a href="/Skill/DeleteSkiil/@x.SkillID" class="btn btn-danger">Sil</a></td>
            <td><a href="/Skill/UpdateSkill/@x.SkillID" class="btn btn-success">Güncelle</a></td>
        </tr>
    }
</table>

<a href="/Skill/CreateSkiil/" class="btn btn-info">YetenekGirisi</a>
